<div class="w-full">
  <div class="bg-blue-700 text-white flex justify-center p-3">
    <p class="font-bold text-xl">Categories</p>
  </div>
  <div>
    <% @category.each do |me|%>
    <div class="flex gap-3">
      <%= image_tag me.icon%>
      <%= me.name%>
    <div> 
    <%end%>
  </div>
</div>
<div class="bg-blue-700 text-white p-3 mx-14 flex justify-center mt-3">
    <%= link_to 'Add a new category', new_category_path%>
  </div>
